  }
  html_join = '<button type="button" onclick="Wo_RegisterGroupJoin(' + id + ', ' + is_confirm_ + ')" class="btn-active btn btn-main btn-mat"><span class="button-text"> <?php echo $wo["lang"]["joined"];?></span></button>';
