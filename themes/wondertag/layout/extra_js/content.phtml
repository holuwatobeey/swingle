function Wo_StripeDonate(fund_id,amount,type = 'stripe') {
  if (!amount || !fund_id) {
    return false;
  }

  if (type == 'stripe') {
    $('.btn-cart').attr('disabled', true).text("<?php echo $wo["lang"]["please_wait"]?>");
  }
  else{
    $('.btn-alipay').attr('disabled', true).text("<?php echo $wo["lang"]["please_wait"]?>");
  }

  

  var handler = StripeCheckout.configure({
    key: "<?php echo $wo['config']['stripe_id'];?>",
    locale: 'english',
    image: '',
    token: function(token) {
    $('.modal-body').html('<i class="fa fa-spin fa-spinner"></i> <?php echo $wo['lang']['c_payment'] ?>');
    $('#pay-go-pro').modal({
      show: true
      });
      $.post(Wo_Ajax_Requests_File() + '?f=funding&s=stripe', {stripeToken:token.id,amount:amount,fund_id:fund_id}, function (data) {
        if (data.status == 200) {
          window.location.href = data.location;
        } else {
          $('.btn-cart').attr('disabled', false).html('<i class="fa fa-spin fa-spinner"></i> <?php echo $wo['lang']['payment_declined'] ?>');
        }
      });
    }
  });

  if (type == 'stripe') {
    handler.open({
      name: "<?php echo $wo['config']['siteName']?>",
      description: "<?php echo $wo['lang']['donate']?>",
      amount: amount*100,
      currency: '<?php echo $wo['config']['stripe_currency']?>',
